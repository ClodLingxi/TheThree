page {
    background-color: #fff;
}

page .round-image {
    border-radius: 12rpx;
}

.side-bar-wrapper {
    display: flex;
    height: 100vh;
}

.side-bar-wrapper .content {
    flex: 1;
}

.side-bar-wrapper .section {
    padding: 32rpx 0;
}

.side-bar-wrapper .title {
    padding-left: 40rpx;
    margin-bottom: 8rpx;
    line-height: 52rpx;
}

.side-bar-wrapper .image {
    width: 96rpx;
    height: 96rpx;
}

.t-side-bar-swapper {
    display: flex;
    flex-direction: column;
}

.view-fab {
    display: flex;
    flex-direction: row;
    position: fixed;
    bottom: 50rpx;
    width: 95%;
    padding: 20rpx;

}

.fab-background {
    display: flex;
    flex-direction: row;
    border: 10rpx;
    border-radius: 10rpx;
    width: 100%;
    height: 100%;
    margin: 10rpx;
    background: grey;
}

